{
    "name": "bokinchan_bbk_plugin",
    "version": "0.1.0",
    "scripts": {
        "old_start": "node scripts/npm-start.js",
        "upload": "kintone-plugin-uploader dist/plugin.zip --watch --waiting-dialog-ms 3000 --base-url https://monosus-dev.cybozu.com/ --username motohashi@monosus.co.jp",
        "develop": "npm run build -- --watch",
        "pack-prod": "kintone-plugin-packer --ppk private_prod.ppk --out dist/plugin_prod.zip plugin",
        "pack-prod-new": "kintone-plugin-packer --out dist/plugin_prod.zip plugin",
        "pack-dev": "kintone-plugin-packer --ppk private_dev.ppk --out dist/plugin_dev.zip plugin",
        "pack-dev-new": "kintone-plugin-packer --out dist/plugin_dev.zip plugin",
        "pack-watch": "kintone-plugin-packer --ppk private.ppk --out dist/plugin.zip plugin --watch plugin",
        "lint": "eslint src",
        "start": "webpack --config webpack.dev.js",
        "watch": "webpack --config webpack.dev.js --watch",
        "build-prod": "webpack --config webpack.prod.js",
        "build-dev": "webpack --config webpack.dev.js",
        "switch-dev": "cp plugin/manifest.dev.json plugin/manifest.json",
        "switch-prod": "cp plugin/manifest.prod.json plugin/manifest.json"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.15.0",
        "@cybozu/eslint-config": "^14.0.2",
        "@kintone/create-plugin": "^4.0.9",
        "@kintone/dts-gen": "^5.0.0",
        "@kintone/plugin-uploader": "^5.0.8",
        "@kintone/webpack-plugin-kintone-plugin": "^5.0.14",
        "@types/bootstrap": "^5.1.9",
        "@types/luxon": "^2.0.9",
        "@types/node": "^16.7.1",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "css-loader": "^6.7.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "fork-ts-checker-webpack-plugin": "^6.3.2",
        "npm-run-all": "^4.1.5",
        "prettier": "2.6.2",
        "regenerator-runtime": "^0.13.9",
        "rimraf": "^3.0.2",
        "style-loader": "^3.3.1",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5",
        "webpack": "^5.51.1",
        "webpack-cli": "^4.8.0"
    },
    "dependencies": {
        "14": "^3.1.6",
        "@kintone/kintone-ui-component": "^0.9.0",
        "@types/micromodal": "^0.3.3",
        "bootstrap": "^5.1.3",
        "buffer": "^6.0.3",
        "encoding-japanese": "^1.0.30",
        "iconv-lite": "^0.6.3",
        "jsqr": "^1.4.0",
        "kintone-ui-component": "^1.3.1",
        "luxon": "^2.3.1",
        "micromodal": "^0.4.10",
        "n": "^7.5.0",
        "ncu": "^0.2.1",
        "nvm": "^0.0.4",
        "util": "^0.12.4"
    },
    "browserslist": [
        "since 2017-06"
    ]
}
